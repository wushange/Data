<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="AppTitle">
        <attr name="showBack" format="boolean" />
        <attr name="canFinish" format="boolean" />
        <attr name="leftButtonRes" format="reference" />
        <attr name="leftImageRes" format="reference" />
        <attr name="leftText" format="string" />
        <attr name="leftTextSize" format="dimension" />
        <attr name="leftTextColor" format="color" />
        <attr name="centerText" format="string" />
        <attr name="centerTextSize" format="dimension" />
        <attr name="centerTextColor" format="color" />
        <attr name="rightText" format="string" />
        <attr name="rightTextSize" format="dimension" />
        <attr name="rightTextColor" format="color" />
        <attr name="rightButtonRes" format="reference" />
        <attr name="rightButton2Res" format="reference" />
    </declare-styleable>
    <attr name="SwipeBackLayoutStyle" format="reference" />
    <declare-styleable name="SwipeBackLayout">
        <attr name="edge_size" format="dimension" />
        <attr name="edge_flag">
            <enum name="left" value="0" />
            <enum name="right" value="1" />
            <enum name="bottom" value="2" />
            <enum name="all" value="3" />
        </attr>
        <attr name="shadow_left" format="reference" />
        <attr name="shadow_right" format="reference" />
        <attr name="shadow_bottom" format="reference" />
    </declare-styleable>
</resources>
